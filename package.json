{
  "name": "next15-starter-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "commit": "git-cz",
    "dev": "next dev --turbopack",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "package:sort": "sort-package-json",
    "prepare": "husky",
    "start": "next start"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm format:fix",
      "pnpm lint:fix"
    ],
    "*.{json,css,md}": [
      "pnpm format:fix"
    ],
    "package.json": [
      "pnpm package:sort"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.476.0",
    "next": "15.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@commitlint/format": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.21.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "commitizen": "^4.3.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "inquirer": "9",
    "lint-staged": "^15.4.3",
    "postcss": "^8",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sort-package-json": "^2.14.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "typescript-eslint": "^8.25.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "overrides": {
    "eslint": "$eslint"
  }
}
